{
    "options" : {
        "method" : "post",
        "print_method" : "server",
        "enctype" : "multipart/form-data",
        "json_validate" : 1,
        "required_mark" : "*",
        "reduce_font_after" : 26
    },
    "fields" : [
        {
            "type" : "file",
            "title" : "&nbsp;",
            "name" : "gallery-images",
            "multiple" : true,
            "limit" : 10,
            "validate" : {
                "default" : true,
                "maxSize" : 1500000,
                "extension" : [
                    "jpg",
                    "jpeg",
                    "png",
                    "gif"
                ]
            },
            "meta_box" : true
        }
    ]
}
